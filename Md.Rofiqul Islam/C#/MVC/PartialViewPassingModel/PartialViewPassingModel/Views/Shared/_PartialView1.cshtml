@using PartialViewPassingModel.Models
@model IEnumerable<PartialViewPassingModel.Models.Product>
<table>
    <tr>
        <th>Product Name</th>
        <th>Product Price</th>
    </tr>
    @foreach (Product p in Model)
    {


        <tr>
            <td>@p.Name</td>
            <td>@p.Price</td>

        </tr>


    }

</table>